def firebase_pods
  pod 'FirebaseAnalytics'
  pod 'FirebaseMessaging'
end

target 'ehome fcm' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  firebase_pods

  # Pods for ehome fcm

  target 'ehome fcmTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ehome fcmUITests' do
    # Pods for testing
  end

end
